<!-- S : dim6 : 학교검색 -->
<div class="dim-content dim-form dim6">
    <h4 class="dim-tit">학교 검색</h4>

    <form action="" method="">
    <table>
        <colgroup>
            <col style="width:80px;">
            <col style="width:auto;">
            <col style="width:100px;">
        </colgroup>
        <tbody>
            <tr>
                <th>&nbsp;</th>
                <td colspan="2">
                    <label class="radio">
                        <input type="radio">
                        <span>중학교</span>
                    </label>
                    <label class="radio">
                        <input type="radio">
                        <span>고등학교</span>
                    </label>
                </td>
            </tr>
            <tr>
                <th>학교 검색</th>
                <td><input type="text" class="input-text" placeholder="키워드를 2자 이상 입력하세요."></td>
                <td><input type="button" class="btn btn-white" value="검색하기"></td>
            </tr>
            <tr>
                <th>학교 선택</th>
                <td colspan="2">
                    <div class="dim-box">
                        <ul>
                            <% For i=1 To 1 %>
                            <li><a href="#">개포고등학교</a></li>
                            <% next %>
                        </ul>
                    </div>
                </td>
            </tr>
        </tbody>
    </table>

    <div class="dim-btn"><button class="btn btn-black btn-confirm">확인</button></div>
    </form>
</div>
