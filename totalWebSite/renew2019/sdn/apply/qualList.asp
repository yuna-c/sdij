<script type="text/javascript">
$(document).ready(function(){
    $('*[data-popup]').click(function(){
        var pop = $(this).attr("id");
        $('.' + pop).show();
        $('body').addClass('on')
    });

    $('.timetable_next').click(function(){
        var num = $('.pop_box img').length;
        var index=$('.pop_box .on').index();
        var next = index +1;

        $('.timetable_prev').removeAttr('disabled');
        if( next == num -1 ){
          $('.timetable_next').attr('disabled','disabled');
          $('.pop_box img').removeClass('on');
          $('.pop_box img').eq(next).addClass('on');
        }else{
          $('.pop_box img').removeClass('on');
          $('.pop_box img').eq(next).addClass('on');
        }
      });

      $('.timetable_prev').click(function(){
        var num = $('.pop_box img').length;
        var index=$('.pop_box .on').index();
        var prev = index -1;

        $('.timetable_next').removeAttr('disabled');

        if( prev == 0){
          $('.timetable_prev').attr('disabled','disabled');
          $('.pop_box img').removeClass('on');
          $('.pop_box img').eq(prev).addClass('on');
        } else{
          $('.pop_box img').removeClass('on');
          $('.pop_box img').eq(prev).addClass('on');
        }
      });

      $('*[data-popup="type1"] .close').click(function(){
        $(this).parents('*[data-popup="type1"]').hide();
        $('body').removeClass('on');
      });


      $('.content-box img').each(function(){
        $(this).on('click',function(){
            var idx = $(this).index();
            $('.pop_box img').eq(idx).addClass('on').siblings('img').removeClass('on')
        });
      })
});
</script>